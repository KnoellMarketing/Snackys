<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Template isFullResponsive="true">
    <Name>Snackys</Name>
    <Author>Knoell Marketing</Author>
    <URL>https://www.knoell-marketing.de</URL>
    <Version>2.07</Version>
    <ShopVersion>406</ShopVersion>
    <Preview>preview.png</Preview>
    <Description>Ultraschnelles responsive JTL Template von Knoell Marketing</Description>
    
	<Settings>
		<Section Name="Allgemein" Key="general">
            <Setting Description="Komprimierung von JavaScript- und CSS-Dateien" Key="use_minify" Type="select" Value="Y">
                <Option Value="Y">Ja (empfohlen)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="JQuery laden" Key="optimize_jquery" Type="select" Value="E">
                <Option Value="I">Inline (Schnell)</Option>
                <Option Value="E">Extern (sofern http2push aktiv ist: schneller)</Option>
            </Setting>
            <Setting Description="JavaScripts optimieren" Key="optimize_all_js" Type="select" Value="N">
                <Option Value="Y">Ja, alle (schneller, Plugins die auf OnLoads reagieren haben ggf. Probleme)</Option>
                <Option Value="YD">Ja, nur bekannte Skripte (am Besten, aber z.B. mit PayPal Payment Wall nicht moeglich)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="Plugin CSS inline laden" Key="optimize_all_css" Type="select" Value="Y">
                <Option Value="Y">Ja (empfohlen)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="Komprimierung von HTML" Key="minify_html" Type="select" Value="Y">
                <Option Value="Y">Ja (Empfohlen)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="Aufgabenplaner-Blindgrafik aktivieren?" Key="use_cron" Type="select" Value="N">
                <Option Value="N">Nein</Option>
                <Option Value="Y">Ja (Nicht empfohlen, besser Cronjobs)</Option>
            </Setting>
            <Setting Description="Sale Badge oder Rabatt in Prozent" Key="saleprozent" Type="select" Value="Y">
                <Option Value="Y">Sale als Prozentzahl ausgeben</Option>
                <Option Value="N">Sale als Begriff (z.B. Angebot) ausgeben</Option>
            </Setting>
            <Setting Description="Bildoptimierung aktivieren" Key="use_image_optimizer" Type="select" Value="Y">
                <Option Value="Y">Ja</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="Bilder in Webp ausgeben" Key="use_image_webp" Type="select" Value="Y">
                <Option Value="Y">Ja</Option>
                <Option Value="N">Nein</Option>
            </Setting>
			<Setting Description="Qualität der Bilder" Key="use_image_quality" Type="text" Value="85"/>
            <Setting Description="Ist Mod Pagespeed aktiv?" Key="mod_pagespeed" Type="select" Value="N">
                <Option Value="Y">Ja</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="Cookie Hinweis aktivieren" Key="use_cookie_hint" Type="select" Value="Y">
                <Option Value="Y">Ja (empfohlen)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="Artikeltexte optimieren" Key="optimize_artikel" Type="select" Value="Y">
                <Option Value="Y">Ja (empfohlen)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="Kategorietexte optimieren" Key="optimize_kategorie" Type="select" Value="Y">
                <Option Value="Y">Ja (empfohlen)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="Seiteninhalte optimieren" Key="optimize_content" Type="select" Value="Y">
                <Option Value="Y">Ja (empfohlen)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
            <Setting Description="News optimieren" Key="optimize_news" Type="select" Value="Y">
                <Option Value="Y">Ja (empfohlen)</Option>
                <Option Value="N">Nein</Option>
            </Setting>
			<Setting Description="Subdomain für JavaScript Ressourcen" Key="cdnDomain" Type="text" Value=""/>
			<Setting Description="Http2 Push Cache Time (Sekunden)" Key="pushCacheTime" Type="text" Value="31536000"/>
			<Setting Description="Templatecopyright entfernen" Key="noCopyright" Type="text" Value=""/>
		</Section>
		<Section Name="gTag: G-Analytics/AdWords/Analytics 360 und Google Tag Manager" Key="analytics">
			<Setting Description="Global Site Tag Tracking-ID 1" Key="id1" Type="text" Value=""/>
			<Setting Description="Global Site Tag Tracking-ID 2" Key="id2" Type="text" Value=""/>
			<Setting Description="Global Site Tag Tracking-ID 3" Key="id3" Type="text" Value=""/>
			<Setting Description="Global Site Tag Tracking-ID 4" Key="id4" Type="text" Value=""/>
			<Setting Description="Global Site Tag Tracking-ID 5" Key="id5" Type="text" Value=""/>
			<Setting Description="Google Tag Manager ID" Key="gtag" Type="text" Value=""/>
            <Setting Description="Artikel ID" Key="artnr" Type="select" Value="artnr">
                <Option Value="artnr">Artikelnummer (cArtNr)</Option>
                <Option Value="id">Interne Artikel id (kArtikel) - Empfohlen wenn die Artikelnummer Sonderzeichen beinhaltet</Option>
            </Setting>
		</Section>
		<Section Name="Theme" Key="theme">
			<Setting Description="Theme" Key="theme_default" Type="select" Value="snackys" override="true">
				<Option Value="snackys">Snackys - with <![CDATA[&hearts;]]> by Knoell Marketing</Option>
				<Option Value="darkmode">Snackys Darkmode - with <![CDATA[&hearts;]]> by Knoell Marketing</Option>
				<Option Value="custom">Custom - eigenes Theme</Option>
			</Setting>
            <Setting Description="Favicon" Key="favicon" Type="upload" Value="favicon.ico" maxFileSize="123456" showPreview="false" allowedFileExtensions="['ico','png']" accept="image/*" target="themes/base/images/" targetFileName="favicon.ico">
			</Setting>
            <Setting Description="Boxed Layout?" Key="designWidth" Type="select" Value="0">
                <Option Value="0">Nein</Option>
                <Option Value="1">Ja</Option>
            </Setting>
            <Setting Description="Boxed Layout: Abstand oben unten" Key="boxedMargin" Type="select" Value="0">
                <Option Value="0">Ja</Option>
                <Option Value="1">Nein</Option>
            </Setting>
            <Setting Description="Boxed Layout: Rahmen anzeigen" Key="boxedBorder" Type="select" Value="0">
                <Option Value="0">Ja</Option>
                <Option Value="1">Nein</Option>
            </Setting>
            <Setting Description="Boxed Layout: Hintergrundbild" Key="boxedImg" Type="text" Value=""/>
            <Setting Description="Zur-Kasse-Button im Sidebar-Warenkorb anzeigen?" Key="shopBButton" Type="select" Value="0">
                <Option Value="0">Ja</Option>
                <Option Value="1">Nein</Option>
            </Setting>
            <Setting Description="Rahmen um Produktbilder? (Listing, Slider, Subkategorien)" Key="productBorder" Type="select" Value="0">
                <Option Value="0">Nein</Option>
                <Option Value="1">Ja</Option>
            </Setting>
			<!--
            <Setting Description="CSS Schriftart PFad" Key="font_path" Type="select" Value="Y">
                <Option Value="Y">Snackys (Standard)</Option>
                <Option Value="custom">Eigenes Theme, Pfad: templates/(child)/themes/(theme)/fonts/fonts.css</Option>
            </Setting>
			-->
            <Setting Description="Schriftart via Google Font laden - z.B.: Source+Sans+Pro:400,600" Key="google_font" Type="text" Value="Source+Sans+Pro:400,600" />
		</Section>
